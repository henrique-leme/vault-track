{
  "version": 2,
  "builds": [
    {
      "src": "dist/src/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/src/schemas/*.graphql"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/src/index.js"
    }
  ],
  "env": {
    "MONGO_URI": "@mongo_uri",
    "DB_NAME": "@db_name",
    "JWT_KEY": "@jwt_key"
  }
}
